@model TBENesys_Orderflow_Core.ViewModels.InvoiceListingViewModel

@{
    ViewData["Title"] = @Model.PageTitle;
    string AllRoles = Model.UserRole;
}

@section styles{
    <link rel="stylesheet" type="text/css" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css" />
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.11.3/css/dataTables.jqueryui.min.css" />
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/buttons/2.0.1/css/buttons.dataTables.min.css" />

    <style>

        .modal-content {
            background-color: #ffffff;
        }

        .modal-dialog {
            max-width: 90%;
        }

        .form-control {
        }

        #dtTablePrinting thead th {
            border-top: 0;
            border-bottom-width: 1px;
            font-family: "Open Sans", sans-serif;
            font-weight: 600;
            font-weight: initial;
            background-color: #D8D8D8;
            border-bottom: 1px solid;
            padding: 10px;
        }

        .main-row {
            background-color: #DFF0D8;
        }

        .alter-row {
            background-color: #E8E8E8;
        }

        .main-row-mail {
            background-color: #D9EDF7;
        }

        .form-control {
            height: 1.875rem;
            border: 2px solid #000;
            padding: 0px 0px 0px 4px !important;
        }

        #dtTable thead th {
            font-size: 11px !important;
        }

        #dtTable tbody td {
            font-size: 11px !important;
            white-space: normal;
        }
    </style>

}
@section Breadcrumbs{
    <h3 class="page-title">@Model.PageTitle</h3>
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href=@Url.Action("Index", "Order" )>Dashboard</a></li>
        </ol>
    </nav>
}
<div class="col-lg-12">
    <div class="card">
        <div class="card-title">
            @if (@ViewBag.ResultMessage != null && @ViewBag.ResultMessage.ToString().Contains("Error"))
            {
                <div class="alert alert-danger">@ViewBag.ResultMessage</div>
            }
            else if (@ViewBag.ResultMessage != null)
            {
                <div class="alert alert-success">@ViewBag.ResultMessage</div>
            }
            @if (!ViewData.ModelState.IsValid)
            {
                <div class="alert alert-danger">
                    <button class="close" data-dismiss="alert"></button>
                    @Html.ValidationSummary()
                </div>
            }
            @if (Model.Response != null)
            {
                @Html.Partial("_MessagePartial", Model.Response)
            }
            <div class="clear"></div>
        </div>
        <div class="card-body">
            <table id="dtTable" class="display table table-bordered table-hover" style="width: 100%; ">
                <thead>
                    <tr>
                        <th></th>
                        <th><b>Order # <br />Order Date<br />Date Mailed</b></th>
                        <th><b>Job Name</b></th>
                        <th><b>Status</b></th>
                        <th text-align:center;"><b>Vendor Invoice <br />Actions</b></th>
                        <th><b>Invoice #</b><br />Invoice Date</th>
                        @if (AllRoles.Contains("32"))
                        {
                            <th style="width: 276px;"><b>Client Invoice <br /> Actions</b></th>
                            <th><b>Client <br />Invoice Date <br /> (Payment Date)</b></th>
                            <th><b>Invoice<br />Recipient</b></th>
                        }
                    </tr>
                </thead>
                <tbody>
                    @if (Model.Records.Count > 0)
                    {


                        foreach (var item in Model.Records)
                        {
                            <tr>
                                <td style=" text-align:center;">
                                    <a style="color: blue; font-size: 20px;" title="View" href="#" onclick="openModal(@item.OrderID)"><i class="mdi mdi-search-web"></i></a>
                                </td>
                                <td>
                                    #: TBEN-@item.OrderID<br />
                                    Order Date: @item.OrderDate<br />
                                    Date Mailed: @item.DateMailed
                                </td>
                                <td>@item.JobName</td>
                                <td style="word-wrap: break-word">@item.Status</td>
                                <td>
                                    @if (item.InvoiceID.HasValue)
                                    {
                                        if (AllRoles.Contains("1") || AllRoles.Contains("32"))
                                        {
                                            <a style="font-size:11px;" title="Edit Invoice" href="#" onclick="openInvoiceModal(@item.OrderID, 'edit')" class="btn btn-danger btn-sm">Edit Invoice</a><br />
                                        }
                                        if (AllRoles.Contains("23") && item.Status == "Pending by vendor")
                                        {
                                            <a style="font-size:11px;" title="Edit Invoice" href="#" onclick="openInvoiceModal(@item.OrderID, 'edit')" class="btn btn-danger btn-sm">Edit Invoice</a><br />
                                        }
                                        if (item.Status == "Submitted by vendor")
                                        {
                                            <a style="font-size: 11px; margin-top: 3px;" title="View PDF" href="@Url.Action("CreateInvoicePDF", "Admin", new { OrderId = item.OrderID })" target="_blank" class="btn btn-dark btn-sm">View PDF</a>
                                        }
                                    }
                                    else
                                    {
                                        <a style="font-size: 11px;" title="Create Invoice" href="#" onclick="openInvoiceModal(@item.OrderID, 'create')" class="btn btn-success btn-sm">Create Invoice</a><br />
                                    }
                                </td>
                                <td>
                                    @item.InvoiceID<br />
                                    @item.InvoiceDate
                                </td>
                                @if (AllRoles.Contains("32"))
                                {
                                    <td>
                                        @if (item.InvoiceID.HasValue)
                                        {
                                            <a style="font-size: 11px;" title="Upload Client Invoice" href="@Url.Action("UploadClientInvoice", "Admin", new { OrderId = item.OrderID })" target="_blank" class="btn btn-primary btn-sm">Upload Client Invoice</a>
                                        }
                                        @if (!String.IsNullOrEmpty(item.ClientInvoiceName))
                                        {
                                            <br />
                                            <a style="font-size: 11px; margin-top: 3px;" title="View PDF" href="@item.ClientInvoiceName" target="_blank" class="btn btn-dark btn-sm">View PDF</a>
                                            <br />
                                            <a style="font-size: 11px; margin-top: 3px;" title="Send Client Invoice" href="@Url.Action("SendClientInvoice", "Admin", new { OrderId = item.OrderID })" class="btn btn-success btn-sm">Send Client Invoice</a>
                                            <br />
                                            <a style="font-size: 11px; margin-top: 3px;" title="Receive Client Payment" href="@Url.Action("ReceiveClientPayment", "Admin", new { OrderId = item.OrderID })" class="btn btn-warning btn-sm">Receive Client Payment</a>
                                        }
                                    </td>
                                    <td>
                                        @item.ClientInvoiceDate<br>
                                        (@item.ClientPaymentDate)
                                    </td>
                                    <td>
                                        @item.InvoiceRecipient
                                    </td>
                                }
                            </tr>
                        }
                    }
                    else
                    {
                        <tr>
                            <td colspan="8">No orders found.</td>
                        </tr>
                    }
                </tbody>
            </table>
